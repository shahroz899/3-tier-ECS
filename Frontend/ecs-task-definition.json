{
    "family": "frontend-task",
    "taskRoleArn": "arn:aws:iam::654654539579:role/ECSExecRole",
    "executionRoleArn": "arn:aws:iam::654654539579:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "512",
    "memory": "1024",
    "containerDefinitions": [
        {
            "name": "frontend",
            "image": "654654539579.dkr.ecr.us-east-1.amazonaws.com/techthree-repo:frontend-latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "frontend-port",
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/frontend-task",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}
